{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Settings": {
    "MongoDb": {
      "ConnectionString": "{CONNECTION_MONGODB}",
      "DatabaseName": "UserDb"
    }
  },
  "JwtSettings": {
    "SecretKey": "{SECRET_KEY}",
    "Issuer": "PulseRegistrationSystem",
    "Audience": "PulseRegistrationSystemUsers",
    "ExpirationMinutes": 60
  }

}


